%\mypsellipse{bighalfaxis}{small/big}
\newcommand{\mypsellipse}[2]{\scalebox{#2 1}{\psarc[linestyle=dashed](0,0){#1}{90}{270}\psarc(0,0){#1}{270}{90}}
}
\def\branecolor{black}%blue
\def\backgroundcolor{black}%green
\def\flowcolor{black}%red


%%
%%Generic brane in parafermion geometry, labels L,L'
%%
\def\parafermionbranemod{
\pspicture(-2.5,-2.5)(2.5,2.5)
%%%Background
\pscircle[linecolor=\backgroundcolor,linewidth=1pt](0,0){2}
%%%Branes
\psset{linecolor=\branecolor,linewidth=2pt}
\qline(1.532088886,1.285575219)(-1,1.732050807)
\psset{dotsize=3pt 0}
\psdots(2,0)(1.532088886,1.285575219)(.3472963546,1.969615506)(-1,1.732050807)(-1.879385242,.6840402852)(-1.879385242,-.6840402866)(-1,-1.732050807)(.3472963600,-1.969615505)(1.532088888,-1.285575217)
\psset{linecolor=black,linewidth=1pt}
\qline(1.532088886,1.285575219)(0,0)
\qline(-1,1.732050807)(0,0)
\psarc(0,0){1}{40}{120}
\psline[linestyle=dashed](0,0)(.2660444426,1.508813014)
\psline[linestyle=dashed](0,0)(2,0)
\psarc[linestyle=dashed](0,0){0.7}{0}{80}
\rput(0.7,-0.6){$\pi \frac{L'}{k}$}
\psline[linewidth=0.5pt](0.5,0.15)(0.6,-0.3)
\rput(-1,0.3){$2 \pi \frac{L}{k}$}
\psline[linewidth=0.5pt](-0.6,0.3)(-0.1,0.7)
\endpspicture
}
%%
%%Generic brane process in parafermion geometry
%%
\def\paraprocI{
\pspicture(-6,-2.5)(6,2.5)
\rput(-3.5,0){%%%Background
\pscircle[linecolor=\backgroundcolor,linewidth=1pt](0,0){2}
%%%Branes
\psset{linecolor=\branecolor,linewidth=2pt}
\qline(2,0)(.3472963546,1.969615506)
\qline(1.532088886,1.285575219)(-1,1.732050807)}
%%%%%%
\rput(3.5,0){%%%Background
\pscircle[linecolor=\backgroundcolor,linewidth=1pt](0,0){2}
%%%Branes
\psset{linecolor=\branecolor,linewidth=2pt}
\qline(2,0)(-1,1.732050807)
\qline(1.532088886,1.285575219)(.3472963546,1.969615506)
}
\psline[linewidth=1.5pt,linecolor=\flowcolor]{->}(-0.8,0)(0.8,0)
\endpspicture
}
