%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Flows in tricritical ising model                          %
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\def\tricriticalisingmodel{
\pspicture(-6,-3)(6,3)
%%%background
\pspolygon[linecolor=\backgroundcolor](-5,-2.5)(-5,2.5)(5,2.5)(5,-2.5)
%%%point-like branes
\psset{linecolor=\branecolor,hatchcolor=\branecolor}
\pscircle*(5,2.5){0.1} %% brane (2,0)
\pscircle*(-5,2.5){0.1}%% brane (0,0)
\pscircle*(0,-2.5){0.1}%% brane (1,0)
%%%2-dim brane (1,1)
\pspolygon[hatchsep=2pt,fillstyle=hlines](-4,0.5)(3.3,0.5)(4,1.5)(-3.3,1.5)
%%%1-dim branes
\psline[linewidth=2pt](-2.2,-1.5)(-1.5,-0.5) %% brane (0,1)
\psline[linewidth=2pt](1.5,-1.5)(2.2,-0.5)   %% brane (0,2)
%%%
%%% 13-Flows
\psset{linecolor=\flowcolor}
\psline{->}(-1.8,-1.1)(-0.2,-2.3) %%Flow (0,1) -> (1,0)
\psline{->}(1.6,-1.2)(0.2,-2.3)   %%Flow (0,2) -> (1,0)
\psline{->}(0,0.3)(0,-2.1)        %%Flow (1,1) -> (1,0)
\psline(0,1.7)(0,1.9)                                       %% Flow
\psline[linearc=0.3]{->}(0,1.9)(0,2.2)(-4.5,2.2)(-4.7,2.35) %% (1,1) ->
\psline[linearc=0.3]{->}(0,1.9)(0,2.2)(4.5,2.2)(4.7,2.35)   %% (0,0)+(2,0)
\psline[linearc=0.5]{->}(-2.0,-0.95)(-4.3,0.2)(-4.8,2.25)%%Flow (0,1)->(0,0)  
\psline[linearc=0.5]{->}(2.0,-0.95)(4.3,0.2)(4.8,2.25) %%Flow (0,2)->(2,0)
%%% Flowlabels 13
\rput(0.6,1.85){$\scriptstyle{+\,}\textstyle\phi_{13}$}
\rput(-1.5,-1.85){$\scriptstyle{+\,}\textstyle\phi_{13}$}
\rput(3.5,-0.6){$\scriptstyle{+\,}\textstyle\phi_{13}$}
\rput(0.6,0){$\scriptstyle{-\,}\textstyle\phi_{13}$}
\rput(1.25,-2){$\scriptstyle{-\,}\textstyle\phi_{13}$}
\rput(-3.5,-0.6){$\scriptstyle{-\,}\textstyle\phi_{13}$}
%%% 12-Flows
\psset{linewidth=2pt,linestyle=dotted}
\psline{->}(-3.8,1.0)(-4.6,2.1)   %%Flow (1,1)->(0,0)
\psline{->}(4,1.5)(4.6,2.1)     %%Flow (1,1)->(2,0)
%%% Flowlabels 12
\rput(-3.85,1.8){$\scriptstyle{-\,}\textstyle\phi_{33}$}
\rput(3.7,1.85){$\scriptstyle{+\,}\textstyle\phi_{33}$}
%%%
%%% Brane labels
\rput(-5,2.8){$\scriptstyle (1,1)$}
\rput(5,2.8){$\scriptstyle (3,1)$}
\rput(0,-2.8){$\scriptstyle (2,1)$}
\rput(-1.2,-0.3){$\scriptstyle (1,2)$}
\rput(2.3,-0.3){$\scriptstyle (1,3)$}
\rput(-2,1){\psframebox*{$\scriptstyle (2,2)$}}
\endpspicture
}

