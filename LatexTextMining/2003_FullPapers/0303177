  \fmf{plain}{v2,v5}
  \fmfv{decor.size=0, label=${\scs 1}$, l.dist=1mm, l.angle=-180}{v1}
  \fmfv{decor.size=0, label=${\scs 6}$, l.dist=1mm, l.angle=0}{v3}
  \fmfv{decor.size=0, label=${\scs 7}$, l.dist=1mm, l.angle=0}{v4}
  \fmfv{decor.size=0, label=${\scs 5}$, l.dist=1mm, l.angle=35}{v5}
  \fmfdot{v2}
  \end{fmfgraph*} } } 
