  \fmf{plain}{v1,v3}
  \fmf{plain}{v2,v4}  
  \fmf{double,width=0.2mm,left=1}{v3,v4,v3}
  \fmfv{decor.size=0, label=${\scs 3}$, l.dist=1mm, l.angle=180}{v1}
  \fmfv{decor.size=0, label=${\scs 5}$, l.dist=1mm, l.angle=0}{v2}
  \end{fmfgraph*} } } 
