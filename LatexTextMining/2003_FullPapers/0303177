  \fmfforce{3.5/14w,1/2h}{v9}
  \fmf{plain}{v1,v9}
  \fmf{double,width=0.2mm}{v2,v9}
  \fmf{plain}{v3,v7}
  \fmf{plain}{v4,v8}
  \fmf{plain,width=0.2mm,left=1}{v5,v7,v5}
  \fmf{plain,left=0.8}{v9,v6}
  \fmf{plain,right=0.8}{v9,v5}
