\eeq
\beq
%\no  \\*[6mm]
\hspace*{2.3cm}+ \hspace*{2mm} \frac{1}{6} \hspace*{3mm}
%%%%                    
  \parbox{17mm}{\centerline{
  \begin{fmfgraph*}(14.5,9)
  \setval
  \fmfforce{0w,1/9h}{v1}
  \fmfforce{0w,8/9h}{v2}
  \fmfforce{1w,1h}{v3}
  \fmfforce{1w,0h}{v4}
