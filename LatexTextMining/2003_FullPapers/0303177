  \fmf{plain}{v2,v3}
  \fmf{plain,right=0.65}{v2,v7}
  \fmf{plain,width=0.2mm,right=1}{v3,v4,v3}
  \fmfv{decor.size=0, label=${\scs 1}$, l.dist=0.5mm, l.angle=-180}{v1}
  \fmfv{decor.size=0, label=${\scs 2}$, l.dist=0.5mm, l.angle=0}{v5}
