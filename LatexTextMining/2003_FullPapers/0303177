  \fmfv{decor.size=0, label=${\scs (2)}$, l.dist=1.5mm, l.angle=-90}{v6}
  \end{fmfgraph*} } }  
%%%%
&=&
\frac{1}{12} \hspace*{3mm}
%%%%
  \parbox{10.5mm}{\centerline{
  \begin{fmfgraph}(8,11)
  \setval
  \fmfforce{0w,3/11h}{v1}
  \fmfforce{1w,3/11h}{v2}
  \fmfforce{0.5w,7/11h}{v3}
  \fmfforce{0.5w,12/11h}{v4}
  \fmf{plain,left=1}{v1,v2,v1}
  \fmf{plain,left=0.4}{v1,v2,v1}
  \fmf{plain,left=1}{v3,v4,v3}
  \fmfdot{v1,v2,v3}
  \end{fmfgraph}}} 
%%%%
\hspace*{3mm} + \hspace*{2mm} \frac{1}{8} \hspace*{2.5mm}
%%%%
  \parbox{20mm}{\centerline{
