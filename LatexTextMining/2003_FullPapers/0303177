  \fmfforce{1w,1/2h}{v2}
  \fmfforce{1/2w,1/2h}{v3}
  \fmf{double,width=0.2mm}{v2,v1}
  \fmfv{decor.size=0, label=${\scs 1}$, l.dist=1mm, l.angle=-180}{v1}
  \fmfv{decor.size=0, label=${\scs 2}$, l.dist=1mm, l.angle=0}{v2}
  \fmfv{decor.size=0, label=${\scs (1)}$, l.dist=1.5mm, l.angle=90}{v3}
  \end{fmfgraph*}}}
%%%%
\hspace*{5mm} = \hspace*{2mm} \frac{1}{2} \hspace*{3mm}
%%%%
  \parbox{13mm}{\centerline{
  \begin{fmfgraph*}(10,5)
