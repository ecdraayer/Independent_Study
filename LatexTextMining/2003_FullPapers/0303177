  \fmfv{decor.size=0, label=${\scs 4}$, l.dist=1mm, l.angle=0}{v2}
  \end{fmfgraph*} } }  
%%
\no \\*[4mm]
%%%%                    %%%%%%%%%%%%%%%%%%%
& &- \hspace*{2mm} 2 \hspace*{3mm} 
%%%%
  \parbox{14mm}{\centerline{
  \begin{fmfgraph*}(11,3)
