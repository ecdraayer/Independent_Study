        _3 ( 0  i R_I ^2 t 2   . )
          _3 ( 0  i t 2 R_i ^2  . ) 

  && 
    [ (_3 (0  i t) _1 ' (0  it)  )^4
      ( _3 ( 0  i ^2 t  ^2  . )
        -1 ) . 

  &&  .
    - (_4 (0  it) _1 ' (0  it)  )^4
      ( _4 ( 0  i ^2 t  ^2  . )
        -1 ) ].
